class Node{
  private String name;
  private int idNum;
  private int gpa;
  
  public Node(String name, int idNum, int gpa){
    this.name = name;
    this.idNum = idNum;
    this.gpa = gpa;
  }
  
  public Node deepCopy(){
    Node node = new Node(name,idNum,gpa);
    return node;
  }
  
  public String to_string(){
   return "Name: " + name + ", ID: " + idNum + ", GPA: " + gpa;
  }
}